body {
	height: 100%;
	width: 100%;
}

img.background {
	height: 100%;
	position: absolute;
	width: 100%;
	z-index: -1;
}

img.gradientv {
	height: 100%;
	opacity: 0.5;
	position: absolute;
	width: 100%;
	z-index: -1;
}

#menu_main h1 {
  color: #ddd;
  display: block;
  font-family: Orbitron;
  font-size: 128px;
  font-weight: bold;
  margin-top: 41%;
  outline-font-effect: outline;
  outline-color: black;
  outline-width: 2px;
  text-align: center;
}
